{
  "name": "vxe-table-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:all && build:doc",
    "build:v1": "cd v1 && npm run build",
    "build:v2": "cd v2 && npm run build",
    "build:v3": "cd v3 && npm run build",
    "build:v3.8": "cd v3.8 && npm run build",
    "build:v4": "cd v4 && npm run build",
    "build:v4_old": "cd v4_old && npm run build",
    "build:v4.6": "cd v4.6 && npm run build",
    "build:other3": "cd other3 && npm run build",
    "build:other4": "cd other4 && npm run build",
    "update": "npm install --legacy-peer-deps && npm run update:all",
    "update:v1": "cd v1 && npm install --legacy-peer-deps",
    "update:v2": "cd v2 && npm install --legacy-peer-deps",
    "update:v3": "cd v3 && npm install --legacy-peer-deps",
    "update:v3.8": "cd v3.8 && npm install --legacy-peer-deps",
    "update:v4": "cd v4 && npm install --legacy-peer-deps",
    "update:v4_old": "cd v4_old && npm install --legacy-peer-deps",
    "update:v4.6": "cd v4.6 && npm install --legacy-peer-deps",
    "update:other3": "cd other3 && npm install --legacy-peer-deps",
    "update:other4": "cd other4 && npm install --legacy-peer-deps",
    "update:all": "npm run update:other3 && npm run update:other4 && npm run update:v1 && npm run update:v2 && npm run update:v3 && npm run update:v3.8 && npm run update:v4 && npm run update:v4_old && npm run update:v4.6",
    "build:all": "npm run build:other3 && npm run build:other4 && npm run build:v1 && npm run build:v2 && npm run build:v3 && npm run build:v3.8 && npm run build:v4 && npm run build:v4_old && npm run build:v4.6",
    "build:docs": "gulp build_docs && npm run build:zip",
    "build:vxe": "gulp build_docs && gulp build_vxe_docs && npm run build:zip",
    "build:zip": "gulp build_zip"
  },
  "devDependencies": {
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-concat": "2.6.1",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.1.4",
    "gulp-zip": "^5.1.0",
    "uglify-js": "^3.17.4"
  }
}
